         url(/web/20141016014016/http://s1.wp.com/wp-content/mu-plugins/genericons/genericons/Genericons.ttf) format('truetype'),
         url(/web/20141016014016/http://s1.wp.com/wp-content/mu-plugins/genericons/genericons/Genericons.svg) format('svg');
    font-weight: normal;
    font-style: normal;
}

